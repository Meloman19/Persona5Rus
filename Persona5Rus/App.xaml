<Application x:Class="Persona5Rus.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:Persona5Rus.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:BooleanNegativeConverter x:Key="BooleanNegativeConverter"/>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:BooleanToVisibilityNegativeConverter x:Key="BooleanToVisibilityNegativeConverter"/>
    </Application.Resources>
</Application>